<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_info_strana"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="8dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="8dp"
    tools:context="com.example.mario.dispositivosmoviles.infoStrana"
    tools:showIn="@layout/activity_info_strana">

    <TextView
        android:text="Strana"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="56dp"
        android:id="@+id/textView96"
        android:textStyle="normal|bold"
        android:textSize="22sp"
        android:textColor="#000" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/strana_ed"
        android:id="@+id/imageView13"
        android:layout_marginBottom="175dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Strana es el lugar donde redefinimos la vida nocturna, un lugar con un ambiente magnético, un espacio donde se viven experiencias únicas e inigualables."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="26dp"
        android:id="@+id/textView97"
        android:textSize="18sp"
        android:layout_alignBottom="@+id/imageView13"
        android:layout_alignParentStart="true"
        android:textColor="#000" />

    <TextView
        android:text="Horario:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView97"
        android:layout_alignParentStart="true"
        android:layout_marginTop="102dp"
        android:id="@+id/textView98"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:textColor="#000" />

    <TextView
        android:text="Jueves a Sabado de 9:00pm a 2:00am"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView98"
        android:layout_alignParentStart="true"
        android:id="@+id/textView99"
        android:textSize="18sp"
        android:textColor="#000" />

    <TextView
        android:text="Direccion:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView99"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp"
        android:id="@+id/textView100"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:textColor="#000" />

    <TextView
        android:text="Río Caura 350, Del Valle, 66220 N.L."
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView100"
        android:layout_alignParentStart="true"
        android:id="@+id/textView101"
        android:textSize="18sp"
        android:textColor="#000" />

    <Button
        android:text="VER EN MAPA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button14" />
</RelativeLayout>
