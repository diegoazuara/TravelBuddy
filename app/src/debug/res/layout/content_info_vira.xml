<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_info_vira"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="8dp"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    android:paddingTop="8dp"
    tools:context="com.example.mario.dispositivosmoviles.infoVira"
    tools:showIn="@layout/activity_info_vira">

    <TextView
        android:text="Vira Vira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="56dp"
        android:id="@+id/textView191"
        android:textStyle="normal|bold"
        android:textSize="22sp"
        android:textColor="#000" />

    <TextView
        android:text="@string/horario_vira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView194"
        android:layout_alignParentStart="true"
        android:id="@+id/textView195"
        android:textSize="18sp"
        android:textColor="#000" />

    <TextView
        android:text="@string/direccion_vira"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView196"
        android:layout_alignParentStart="true"
        android:id="@+id/textView197"
        android:textSize="18sp"
        android:textColor="#000" />

    <TextView
        android:text="@string/horario"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:id="@+id/textView194"
        android:textStyle="normal|bold"
        android:textSize="19sp"
        android:textColor="#000"
        android:layout_below="@+id/imageView29"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/viravira_ed"
        android:id="@+id/imageView29"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="31dp" />

    <TextView
        android:text="@string/vira_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="40dp"
        android:id="@+id/textView193"
        android:textSize="18sp"
        android:textColor="#000"
        android:layout_above="@+id/textView195"
        android:layout_alignParentStart="true" />

    <Button
        android:text="@string/ver_mapa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/button30" />

    <TextView
        android:text="@string/direccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:id="@+id/textView196"
        android:textStyle="normal|bold"
        android:textSize="19sp"
        android:textColor="#000"
        android:layout_below="@+id/textView195"
        android:layout_alignParentStart="true" />
</RelativeLayout>
