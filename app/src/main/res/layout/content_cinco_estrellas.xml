<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <RelativeLayout
        android:layout_height="match_parent"
        android:paddingBottom="8dp"
        android:paddingLeft="8dp"
        android:paddingRight="8dp"
        android:paddingTop="8dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context=".cincoEstrellas"
        tools:showIn="@layout/activity_cinco_estrellas"
        android:layout_width="match_parent"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true">

        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/live_aqua"
            android:src="@drawable/live_aqua"
            android:background="#fff"
            android:scaleType="fitXY"
            android:padding="2dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_marginTop="56dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Live Aqua Valle"
            android:id="@+id/textView149"
            android:paddingTop="60dp"
            android:textColor="#fff"
            android:textSize="25dp"
            android:background="#1a6bff"
            android:textAlignment="center"
            android:layout_alignTop="@+id/live_aqua"
            android:layout_alignParentEnd="true"
            android:layout_toEndOf="@+id/live_aqua"
            android:layout_above="@+id/textView150"
            android:layout_margin="2dp"
            android:width="100dp" />

        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/fiesta_inn"
            android:src="@drawable/fiesta_inn"
            android:background="#fff"
            android:scaleType="fitXY"
            android:padding="2dp"
            android:layout_below="@+id/live_aqua"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Fiesta Inn"
            android:id="@+id/textView150"
            android:paddingTop="60dp"
            android:textColor="#fff"
            android:textSize="25dp"
            android:background="#4d8cff"
            android:textAlignment="center"
            android:layout_alignTop="@+id/fiesta_inn"
            android:layout_alignBottom="@+id/fiesta_inn"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:layout_alignParentEnd="true"
            android:layout_toEndOf="@+id/fiesta_inn" />





        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/safi"
            android:src="@drawable/safi"
            android:background="#fff"
            android:scaleType="fitXY"
            android:padding="2dp"
            android:layout_below="@+id/fiesta_inn"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Safi"
            android:id="@+id/textView151"
            android:paddingTop="60dp"
            android:textColor="#fff"
            android:textSize="25dp"
            android:background="#1a6bff"
            android:textAlignment="center"
            android:layout_alignTop="@+id/safi"
            android:layout_alignBottom="@+id/safi"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:layout_alignParentEnd="true"
            android:layout_toEndOf="@+id/safi" />

        <ImageButton
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/krystal"
            android:src="@drawable/krystal"
            android:background="#fff"
            android:scaleType="fitXY"
            android:padding="2dp"
            android:layout_below="@+id/safi"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Krystal"
            android:id="@+id/textView152"
            android:paddingTop="60dp"
            android:textColor="#fff"
            android:textSize="25dp"
            android:background="#4d8cff"
            android:textAlignment="center"
            android:layout_alignTop="@+id/krystal"
            android:layout_alignBottom="@+id/krystal"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:layout_alignParentEnd="true"
            android:layout_toEndOf="@+id/krystal" />



    </RelativeLayout>
</ScrollView>