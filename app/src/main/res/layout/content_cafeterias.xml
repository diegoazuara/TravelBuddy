<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools">

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cafeterias"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.mario.dispositivosmoviles.cafeterias">

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/thenada"
        android:src="@drawable/thenada"
        android:background="#fff"
        android:scaleType="fitXY"
        android:padding="2dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="56dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="The Nada"
        android:id="@+id/textView32"
        android:paddingTop="60dp"
        android:textColor="#fff"
        android:textSize="25dp"
        android:background="#6d4c41"
        android:textAlignment="center"
        android:layout_alignTop="@+id/thenada"
        android:layout_alignBottom="@+id/thenada"
        android:layout_margin="2dp"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/thenada"/>

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/tierralibre"
        android:src="@drawable/tierralibre"
        android:background="#fff"
        android:scaleType="fitXY"
        android:padding="2dp"
        android:layout_below="@+id/thenada"
        android:layout_alignParentStart="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tierra Libre"
        android:id="@+id/textView33"
        android:paddingTop="60dp"
        android:textColor="#fff"
        android:textSize="25dp"
        android:background="#b71c1c"
        android:textAlignment="center"
        android:layout_alignTop="@+id/tierralibre"
        android:layout_alignBottom="@+id/tierralibre"
        android:layout_margin="2dp"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/tierralibre"/>

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/lagaleria"
        android:src="@drawable/lagaleria"
        android:background="#fff"
        android:scaleType="fitXY"
        android:padding="2dp"
        android:layout_below="@+id/tierralibre"
        android:layout_alignParentStart="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="La Galería Café"
        android:id="@+id/textView34"
        android:paddingTop="58dp"
        android:textColor="#fff"
        android:textSize="25dp"
        android:background="#6d4c41"
        android:textAlignment="center"
        android:layout_alignTop="@+id/lagaleria"
        android:layout_alignBottom="@+id/lagaleria"
        android:layout_margin="2dp"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/lagaleria"/>

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/puntadelcielo"
        android:src="@drawable/puntadelcielo"
        android:background="#fff"
        android:scaleType="fitXY"
        android:padding="2dp"
        android:layout_below="@+id/lagaleria"
        android:layout_alignParentStart="true"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Café Punta del Cielo"
        android:id="@+id/textView35"
        android:paddingTop="60dp"
        android:textColor="#fff"
        android:textSize="25dp"
        android:background="#b71c1c"
        android:textAlignment="center"
        android:layout_alignTop="@+id/puntadelcielo"
        android:layout_alignBottom="@+id/puntadelcielo"
        android:layout_margin="2dp"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/puntadelcielo"/>


</RelativeLayout>
</ScrollView>